<odoo>
    <data>
        <record id="crm_lead_bidding_format_action" model="ir.actions.act_window">
            <field name="name">Formato Licitaci√≥n</field>
            <field name="res_model">crm.lead.bidding.format</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="crm_lead_bidding_format_tree_view" model="ir.ui.view">
            <field name="name">crm.lead.bidding.format.tree.view</field>
            <field name="model">crm.lead.bidding.format</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="crm_lead_bidding_format_form_view" model="ir.ui.view">
            <field name="name">crm.lead.bidding.format.form.view</field>
            <field name="model">crm.lead.bidding.format</field>
            <field name="arch" type="xml">
                <form create="false">
                    <sheet>
                        <div class="oe_title">
                            <h1>
                                <label for="name"/>
                                <field name="name" string="Estado"/>
                            </h1>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>