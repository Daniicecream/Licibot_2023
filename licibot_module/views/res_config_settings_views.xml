<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_config_settings_view_form_inherit_licibot" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.licibot</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='integration']" position="after">
                <h2>Licibot</h2>
                <div class="content-group mt16" name="licibot">
                    <group>
                        <label for="token_api_mp"/>
                        <div>
                            <field name="token_api_mp"/>
                        </div>
                    </group>
                </div>
            </xpath>
        </field>
    </record>
    
</odoo>
